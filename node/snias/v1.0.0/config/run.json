{
    "environment": "development",
    "sock": {
        "listen": {
            "host": "localhost",
            "port": 5000
        },
        "client_url": "http://cdn.sockjs.org/sockjs-0.3.min.js",
        "response_limit": 128000,
        "websocket": true,
        "jsessionid": true,
        "heartbeat_delay": 15000,
        "disconnect_delay": 5000,
        "prefix": "/echo"
    }
}
